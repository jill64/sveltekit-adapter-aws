{
  "name": "@jill64/sveltekit-adapter-aws",
  "description": "AWS adapter for SvelteKit with multiple architecture",
  "version": "0.4.37",
  "type": "module",
  "main": "dist/index.js",
  "license": "MIT",
  "author": "jill64 <intents.turrets0h@icloud.com> (https://github.com/jill64)",
  "bugs": "https://github.com/jill64/sveltekit-adapter-aws/issues",
  "homepage": "https://github.com/jill64/sveltekit-adapter-aws#readme",
  "files": [
    "dist",
    "embed",
    "cdk"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "keywords": [
    "svelte",
    "aws-cdk"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jill64/sveltekit-adapter-aws.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "pnpm build:adapter && cd packages/site && pnpm dev",
    "build": "pnpm build:adapter && pnpm build:site && npx publint",
    "build:adapter": "pnpm transpile:embed && pnpm build:cf2 && cd packages/adapter && pnpm build",
    "build:cf2": "npx tsx cf2/scripts/build.ts",
    "transpile:embed": "cd embed && pnpm run transpile",
    "build:site": "cd packages/site && pnpm build",
    "preview": "pnpm build:adapter && cd packages/site && pnpm preview",
    "check": "tsc && cd packages/site && pnpm check",
    "test": "playwright test",
    "prepack": "npm run build:adapter",
    "format": "npx prettier --write . --plugin prettier-plugin-svelte .",
    "lint": "npx prettier --check . --plugin prettier-plugin-svelte . && npx eslint"
  },
  "peerDependencies": {
    "@sveltejs/kit": "^1.0.0"
  },
  "devDependencies": {
    "@playwright/test": "1.39.0",
    "@typescript-eslint/eslint-plugin": "6.7.5",
    "@typescript-eslint/parser": "6.7.5",
    "cf2-builder": "0.0.5",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-svelte": "2.34.0",
    "prettier-plugin-svelte": "3.0.3"
  },
  "dependencies": {
    "@jill64/types-lambda": "0.1.15",
    "@sveltejs/kit": "1.25.2",
    "@types/mime-types": "2.1.2",
    "@types/node": "20.8.4",
    "aws-cdk": "2.100.0",
    "aws-cdk-lib": "2.100.0",
    "constructs": "10.3.0",
    "dotenv": "16.3.1",
    "esbuild": "0.19.4",
    "mime-types": "2.1.35",
    "nanoid": "5.0.1",
    "typescript": "5.2.2"
  }
}
