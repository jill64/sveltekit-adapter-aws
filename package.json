{
  "name": "@jill64/sveltekit-adapter-aws",
  "description": "AWS adapter for SvelteKit with multiple architecture",
  "version": "0.4.5",
  "type": "module",
  "main": "dist/index.js",
  "license": "MIT",
  "author": "jill64 <intents.turrets0h@icloud.com> (https://github.com/jill64)",
  "bugs": "https://github.com/jill64/sveltekit-adapter-aws/issues",
  "homepage": "https://github.com/jill64/sveltekit-adapter-aws#readme",
  "files": [
    "dist",
    "embed",
    "cdk"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "keywords": [
    "svelte",
    "aws-cdk"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jill64/sveltekit-adapter-aws.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "pnpm build:adapter && cd packages/site && pnpm dev",
    "build": "pnpm build:adapter && pnpm build:site && publint",
    "build:adapter": "pnpm transpile:embed && cd packages/adapter && pnpm build",
    "transpile:embed": "cd embed && pnpm run transpile",
    "build:site": "cd packages/site && pnpm build",
    "preview": "pnpm build:adapter && cd packages/site && pnpm preview",
    "check": "tsc && cd packages/site && pnpm check",
    "test": "playwright test",
    "format": "prettier --write . --plugin prettier-plugin-svelte .",
    "lint": "prettier --check . --plugin prettier-plugin-svelte . && eslint ."
  },
  "peerDependencies": {
    "@sveltejs/kit": "^1.0.0"
  },
  "devDependencies": {
    "@playwright/test": "1.38.1",
    "@typescript-eslint/eslint-plugin": "6.7.3",
    "@typescript-eslint/parser": "6.7.3",
    "eslint": "8.50.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-svelte": "2.33.2",
    "prettier": "3.0.3",
    "prettier-plugin-svelte": "3.0.3",
    "publint": "0.2.3"
  },
  "dependencies": {
    "@jill64/types-lambda": "^0.1.8",
    "@sveltejs/kit": "1.25.1",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^20.6.2",
    "aws-cdk": "^2.96.2",
    "aws-cdk-lib": "2.97.1",
    "constructs": "^10.2.70",
    "dotenv": "^16.3.1",
    "esbuild": "^0.19.3",
    "mime-types": "^2.1.35",
    "nanoid": "^5.0.1",
    "tsx": "^3.12.10",
    "typescript": "5.2.2"
  }
}
