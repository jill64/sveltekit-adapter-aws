{
  "name": "@hearchco/sveltekit-adapter-aws",
  "description": "SvelteKit AWS adapter with multiple architectures (only adapter, no deployment)",
  "version": "0.0.1",
  "type": "module",
  "main": "dist/index.js",
  "license": "MIT",
  "author": {
    "name": "Hearchco",
    "email": "hello@hearch.co",
    "url": "https://github.com/hearchco",
    "image": "https://avatars.githubusercontent.com/u/152706942?s=200&v=4"
  },
  "bugs": "https://github.com/hearchco/sveltekit-adapter-aws/issues",
  "homepage": "https://github.com/hearchco/sveltekit-adapter-aws#readme",
  "files": [
    "dist",
    "embed",
    "cdk"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "keywords": [
    "adapter",
    "aws",
    "sveltekit"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hearchco/sveltekit-adapter-aws.git",
    "image": "https://opengraph.githubassets.com/c32f66289531733b2f960e3b9bc9f3d2bfa03a8e056cf4068888b2d2d9a32ecf/hearchco/sveltekit-adapter-aws"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build:all": "pnpm build:adapter && pnpm build:site && npx publint",
    "build:adapter": "pnpm transpile:embed && pnpm build:adapterOnly",
    "build:adapterOnly": "cd packages/adapter && pnpm build",
    "transpile:embed": "cd embed && pnpm transpile",
    "prepack": "pnpm build:adapter",
    "format": "pnpm format:lib && pnpm format:site",
    "format:lib": "npx prettier -w .",
    "lint": "pnpm lint:lib && pnpm lint:embed",
    "lint:lib": "npx eslint . && tsc",
    "lint:embed": "cd embed && pnpm lint"
  },
  "peerDependencies": {
    "@sveltejs/kit": "^2.0.0"
  },
  "devDependencies": {
    "@jill64/eslint-config-ts": "1.1.18",
    "@jill64/prettier-config": "1.0.0",
    "@jill64/playwright-config": "2.2.5",
    "@sveltejs/kit": "2.5.6",
    "@types/node": "20.12.7",
    "typescript": "5.4.5"
  },
  "dependencies": {
    "@jill64/types-lambda": "1.1.4",
    "@types/mime-types": "2.1.4",
    "tsx": "4.7.2",
    "constructs": "10.3.0",
    "dotenv": "16.4.5",
    "esbuild": "0.20.2",
    "mime-types": "2.1.35",
    "nanoid": "5.0.7"
  },
  "prettier": "@hearchco/prettier-config"
}
